{
	"rules": {
		"class-name": true,
		"comment-format": [
			true,
			"check-space"
		],
		"no-var-keyword": true,
		"semicolon": [
			true,
			"always",
			"ignore-bound-class-methods"
		],
		"no-internal-module": true,
		"no-trailing-whitespace": true,
		"no-null-keyword": true,
		"prefer-const": true,
		"jsdoc-format": true
	}
}